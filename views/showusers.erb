<table class="table table-striped">
<thead>
  	<tr>
 		<th>Имя</th>
 		<th>Телефон</th>
 		<th>Дата и время</th>
 		<th>Барбер</th>
 		<th>Цвет</th>
 	</tr>
</thead>

<% @results.each do |row| %>

<tbody>
 	<tr>
 		<td><%= row['username'] %></td>
 		<td><%= row['phone'] %></td>
 		<td><%= row['date_stamp'] %></td>
 		<td><%= row['barber'] %></td>
 		<td><%= row['color'] %></td>
 	</tr>
<% end %>
</tbody>

</table>



